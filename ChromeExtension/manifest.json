{
  "manifest_version": 3,
  "name": "Keka Token Extractor",
  "version": "1.0",
  "permissions": [
    "scripting",
    "activeTab",
    "notifications"
  ],
  "host_permissions": ["<all_urls>"],
  "action": {
    "default_title": "Extract Token"
  },
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "16": "icon.png",
    "32": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  }
}
